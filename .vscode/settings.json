        "padx",
        "pady",
        "pyautogui",
        "pygame",
        "Raycasting",
        "raycasts",
        "screeninfo",
        "sonner",
        "SOUNDPACK",
        "SOUNDPACKS",
        "uids",
        "ultralytics",
        "visualisation",
        "webserver",
        "Websockets",
        "xyxy",
        "yolo",
        "YOLO",
        "yolov"
