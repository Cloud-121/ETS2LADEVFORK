    "godotTools.editorPath.godot4": "f:\\Godot\\Godot_v4.3-stable_win64.exe",
    "python.languageServer": "Pylance",
    "python.analysis.diagnosticSeverityOverrides": {
        "reportMissingModuleSource": "none",
        "reportShadowedImports": "none"
    },
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/__pycache__": true,
        ".idea": true,
        ".vscode": true,
        ".github": true,
        "cache": true,
        "logs": true,
        "venv": true
    },
    "hide-files.files": []
}